
public class SinglyLinkedList {
	Node head;

	public void add(int data) {
		Node trav;
		
		Node node = new Node();
		node.data = data;
		node.next = null;
		
		if(head == null) {
			head = node;
		}
		else {
			trav = head;
			while(trav.next != null) {
				trav = trav.next;
			}
			trav.next = node;
		}
	}
	
	public void add(int data, int position) {
		
	}
	
	
}
